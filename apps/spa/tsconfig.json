{
  
  "extends": "../../shared-configs/tsconfig.json",
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "jsx": "react",
    "moduleResolution": "node",
    "isolatedModules": true,
    "allowJs": true,
    "noImplicitAny": true,
    "alwaysStrict": true,
    "baseUrl": "./src",
    "paths": {
      "@/*": ["*","*.ts", "*.tsx"],
    },
  },
  "include": ["src/**/*.ts", "src/**/*.tsx"],
  "exclude": ["node_modules"],
}
